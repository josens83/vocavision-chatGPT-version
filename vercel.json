{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "vocavision",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://vocavisionbackend-production.up.railway.app/api"
  },
  "regions": ["icn1"]
}
