# Base URLs
NEXT_PUBLIC_APP_URL=https://vocavision.vercel.app
NEXT_PUBLIC_API_URL=https://vocavisionbackend-production.up.railway.app/api

# OAuth providers
NEXT_PUBLIC_KAKAO_CLIENT_ID=
NEXT_PUBLIC_KAKAO_REDIRECT_URI=${NEXT_PUBLIC_APP_URL}/auth/callback/kakao
NEXT_PUBLIC_GOOGLE_CLIENT_ID=
NEXT_PUBLIC_GOOGLE_REDIRECT_URI=${NEXT_PUBLIC_APP_URL}/auth/callback/google

# Payments / analytics / monitoring
NEXT_PUBLIC_TOSS_CLIENT_KEY=
NEXT_PUBLIC_GA_MEASUREMENT_ID=
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_APP_VERSION=1.0.0

# Media & notifications
NEXT_PUBLIC_CDN_URL=
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=
NEXT_PUBLIC_CLOUDINARY_API_KEY=
NEXT_PUBLIC_VAPID_PUBLIC_KEY=

# Feature flags
# Toggle the post-auth quick start flow across environments
NEXT_PUBLIC_QUICK_START_ENABLED=true
NEXT_PUBLIC_USE_MOCK_API=false
